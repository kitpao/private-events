<h1 class= "title"> <%= @user.name %>'s profile page! </h1>

<h2 class= "subtitle"> My created events: </h2>
<ul>
  <% @user.created_events.each do |event| %>
    <li><%= link_to event.description, event_path(event) %></li>
  <% end %>
</ul>


<h2 class= "subtitle"> My attended events: </h2>

<ul>
  <% @user.attended_events.each do |event| %>
    <li><%= link_to event.description, event_path(event) %></li>
  <% end %>
</ul>